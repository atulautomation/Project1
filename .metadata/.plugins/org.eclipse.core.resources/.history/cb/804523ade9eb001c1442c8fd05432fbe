package preexam;

import java.util.Iterator;

public class Harshda
{

	public static void main(String[] args)
	{
		int[] a= {1,1,1,2,2,3,5,5,5,6,4};
		int[] b= {1,2,3};
		Code(a,b);
	}
	private static void Code(int[] a,int[] b)
	{
		for(int i=0;i<b.length;i++)
		{	
			int ele=b[i];
			int cnt=0;
			for (int j=0;j<a.length;j++)
			{
				if(a[j]==ele)
					cnt++;
			}
			if(cnt==2)
				break;
		
			int[] k=new int[a.length-cnt];
		
			int index=0;
			int cnt1=0;
			for(int j=0;j<a.length;j++)
			{
				if(a[j]!=ele)
				{	
					k[index++]=a[i];
				}
				else if(a[j]==ele && cnt1==2)
				{	
					k[index++]=a[i];
				}
				else
					cnt1++;
			}
			a=k;
		}
		for(int j=0;j<a.length;j++)
		{
			System.out.print(a[j]+" ");
		}
	}
}	
		
		

